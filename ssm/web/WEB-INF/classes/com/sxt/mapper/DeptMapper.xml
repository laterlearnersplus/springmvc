<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sxt.mapper.DeptMapper">
	<select id="query" resultType="com.sxt.entity.Dept">
		SELECT * FROM dept
	</select>
	<select id="queryById" parameterType="int" resultType="com.sxt.entity.Dept">
		SELECT * FROM dept WHERE id=#{id}
	</select>
</mapper>